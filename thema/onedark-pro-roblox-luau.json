  "tokenColors": [
    {
      "name": "Global settings",
      "settings": {
        "background": "#282c34",
        "foreground": "#abb2bf",
        "fontStyle": ""
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "comment.line",
        "comment.block",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#5c6370",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Constants and Enumeration Values",
      "scope": [
        "variable.other.constant",
        "variable.other.constant.declaration",
        "constant.language.luau",
        "constant.numeric",
        "constant.boolean",
        "constant.character",
        "constant.other",
        "support.constant"
      ],
      "settings": {
        "foreground": "#d19a66",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Properties and Object Keys",
      "scope": [
        "variable.other.property",
        "variable.object.property",
        "meta.object-literal.key",
        "meta.field.declaration"
      ],
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": ""
      }
    },
    {
      "name": "Types and Type Annotations",
      "scope": [
        "entity.name.type",
        "support.type",
        "meta.type.annotation",
        "meta.type.generic",
        "storage.type.generic",
        "storage.type.annotation",
        "storage.type",
        "keyword.operator.type",
        "punctuation.definition.type"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Type Parameters",
      "scope": [
        "variable.parameter.type"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions and Methods",
      "scope": [
        "entity.name.function",
        "meta.function.declaration",
        "meta.function",
        "support.function",
        "keyword.function",
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#61afef",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Function Calls",
      "scope": [
        "meta.function-call",
        "variable.function",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#61afef",
        "fontStyle": ""
      }
    },
    {
      "name": "Function Parameters",
      "scope": [
        "variable.parameter",
        "meta.function.parameters"
      ],
      "settings": {
        "foreground": "#e06c75",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Roblox Services and API",
      "scope": [
        "support.class.roblox",
        "support.function.roblox",
        "variable.language.roblox",
        "support.function.service.roblox",
        "meta.function-call.roblox"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Strings and Template Literals",
      "scope": [
        "string.quoted",
        "string.template",
        "punctuation.definition.string",
        "string.interpolated"
      ],
      "settings": {
        "foreground": "#98c379",
        "fontStyle": ""
      }
    },
    {
      "name": "Keywords and Control Flow",
      "scope": [
        "keyword.control",
        "keyword.operator",
        "storage.modifier",
        "keyword.local",
        "keyword.return",
        "keyword.if",
        "keyword.else",
        "keyword.elseif",
        "keyword.end",
        "keyword.while",
        "keyword.for",
        "keyword.do",
        "keyword.break",
        "keyword.continue",
        "keyword.in",
        "keyword.import",
        "keyword.export"
      ],
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Meta Keywords",
      "scope": [
        "punctuation.definition.directive.luau",
        "keyword.directive.luau"
      ],
      "settings": {
        "foreground": "#c678dd",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Variables and Identifiers",
      "scope": [
        "variable.declaration",
        "variable.name",
        "variable.other",
        "variable.language"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Global Variables",
      "scope": [
        "variable.other.global"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": "italic bold"
      }
    },
    {
      "name": "Class Names and Constructor Methods",
      "scope": [
        "entity.name.class",
        "new.expr entity.name.type",
        "meta.class entity.name.type",
        "entity.other.inherited-class"
      ],
      "settings": {
        "foreground": "#e5c07b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Luau Modules and Requires",
      "scope": [
        "meta.import",
        "meta.require"
      ],
      "settings": {
        "foreground": "#61afef",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.logical",
        "keyword.operator.comparison",
        "keyword.operator.concat.luau"
      ],
      "settings": {
        "foreground": "#56b6c2",
        "fontStyle": ""
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.accessor",
        "punctuation.definition.parameters",
        "punctuation.definition.block"
      ],
      "settings": {
        "foreground": "#abb2bf"
      }
    },
    {
      "name": "Metatable Keys",
      "scope": [
        "meta.metatable.key"
      ],
      "settings": {
        "foreground": "#98c379",
        "fontStyle": "italic bold"
      }
